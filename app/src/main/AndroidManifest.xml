<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chhd.android.demo">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="主界面">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PopupActivity" />
        <activity android:name=".RvListActivity" />
        <activity android:name=".RvGridActivity" />
        <activity android:name=".ClickBgActivity" />
        <activity android:name=".ClearEditTextActivity" />
        <activity android:name=".ListDemoActivity" />
        <activity android:name=".ProgressDemoActivity" />
        <activity android:name=".FragmentListActivity" />
        <activity android:name=".SpUtilsActivity" />
        <activity android:name=".CompoundActivity" />
        <activity android:name=".FragmentTabActivity"></activity>
    </application>

</manifest>